{
  "name": "nestjs-openapi-static",
  "version": "2.2.0",
  "description": "Static code analysis tool to generate OpenAPI specifications from NestJS applications",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "import": "./dist/internal.mjs",
      "default": "./dist/internal.mjs"
    }
  },
  "bin": {
    "nestjs-openapi-static": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "typecheck": "tsc --noEmit && tsc --noEmit -p e2e-applications",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run test/openapi-e2e-apps.test.ts",
    "publint": "publint",
    "knip": "knip",
    "prepublishOnly": "npm run build && npm run publint",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "keywords": [
    "nestjs",
    "openapi",
    "swagger",
    "typescript",
    "static-analysis"
  ],
  "author": "Eliya Cohen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Newbie012/nestjs-openapi-static.git"
  },
  "bugs": {
    "url": "https://github.com/Newbie012/nestjs-openapi-static/issues"
  },
  "homepage": "https://github.com/Newbie012/nestjs-openapi-static#readme",
  "packageManager": "pnpm@10.17.0",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^11.1.6",
    "@types/js-yaml": "^4.0.9",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.18.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "class-validator": "^0.14.3",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "knip": "^5.82.1",
    "prettier": "^3.0.0",
    "publint": "^0.3.17",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.54.0",
    "unbuild": "^3.6.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "effect": "^3.19.15",
    "glob": "^13.0.0",
    "js-yaml": "^4.1.1",
    "minimist": "^1.2.8",
    "ts-json-schema-generator": "^2.4.0",
    "ts-morph": "^27.0.2",
    "tsx": "^4.20.5"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "@nestjs/swagger": "^7.0.0 || ^8.0.0 || ^11.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/core": {
      "optional": true
    },
    "@nestjs/swagger": {
      "optional": true
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
