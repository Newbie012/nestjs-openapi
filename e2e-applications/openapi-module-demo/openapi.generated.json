{
  "components": {
    "schemas": {
      "CreateProductDto": {
        "description": "DTO for creating a new product",
        "properties": {
          "category": {
            "$ref": "#/components/schemas/ProductCategory"
          },
          "description": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "tags": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "name",
          "price",
          "category"
        ],
        "type": "object"
      },
      "ProductCategory": {
        "description": "Product category enum",
        "enum": [
          "electronics",
          "clothing",
          "food",
          "other"
        ],
        "type": "string"
      },
      "ProductCreatedEvent": {
        "description": "Event when a product is created",
        "properties": {
          "name": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "productId": {
            "type": "string"
          },
          "timestamp": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "required": [
          "type",
          "productId",
          "name",
          "price",
          "timestamp"
        ],
        "type": "object"
      },
      "ProductDeletedEvent": {
        "description": "Event when a product is deleted",
        "properties": {
          "productId": {
            "type": "string"
          },
          "timestamp": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "required": [
          "type",
          "productId",
          "timestamp"
        ],
        "type": "object"
      },
      "ProductDto": {
        "description": "Product DTO for the demo API Using interfaces to prove static analysis works without runtime metadata",
        "properties": {
          "category": {
            "$ref": "#/components/schemas/ProductCategory"
          },
          "description": {
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "tags": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "id",
          "name",
          "price",
          "category",
          "tags"
        ],
        "type": "object"
      },
      "ProductEvent": {
        "anyOf": [
          {
            "$ref": "#/components/schemas/ProductCreatedEvent"
          },
          {
            "$ref": "#/components/schemas/ProductUpdatedEvent"
          },
          {
            "$ref": "#/components/schemas/ProductDeletedEvent"
          }
        ],
        "description": "Union type of all product events This demonstrates that the library can handle discriminated unions"
      },
      "ProductUpdatedEvent": {
        "description": "Event when a product is updated",
        "properties": {
          "changes": {
            "properties": {
              "category": {
                "$ref": "#/components/schemas/ProductCategory"
              },
              "name": {
                "type": "string"
              },
              "price": {
                "type": "number"
              }
            },
            "type": "object"
          },
          "productId": {
            "type": "string"
          },
          "timestamp": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "required": [
          "type",
          "productId",
          "changes",
          "timestamp"
        ],
        "type": "object"
      }
    }
  },
  "info": {
    "description": "Demo API showcasing OpenApiModule with interface-based DTOs",
    "title": "Products API",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {
    "/products": {
      "get": {
        "operationId": "ProductController_findAll",
        "parameters": [],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ProductDto"
                  },
                  "type": "array"
                }
              }
            },
            "description": "List of products"
          }
        },
        "summary": "Get all products",
        "tags": [
          "products"
        ]
      },
      "post": {
        "operationId": "ProductController_create",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateProductDto"
              }
            }
          },
          "description": "Request body parameter: createProductDto",
          "required": true
        },
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductDto"
                }
              }
            },
            "description": "Product created"
          }
        },
        "summary": "Create a new product",
        "tags": [
          "products"
        ]
      }
    },
    "/products/{id}": {
      "get": {
        "operationId": "ProductController_findOne",
        "parameters": [
          {
            "description": "path parameter: id",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductDto"
                }
              }
            },
            "description": "The product"
          },
          "404": {
            "description": "Product not found"
          }
        },
        "summary": "Get a product by ID",
        "tags": [
          "products"
        ]
      }
    },
    "/products/{id}/events": {
      "get": {
        "operationId": "ProductController_getEvents",
        "parameters": [
          {
            "description": "path parameter: id",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "description": "query parameter: limit",
            "in": "query",
            "name": "limit",
            "required": false,
            "schema": {
              "oneOf": [
                {
                  "type": "number"
                },
                {
                  "type": "object"
                }
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ProductEvent"
                  },
                  "type": "array"
                }
              }
            },
            "description": "List of product events"
          }
        },
        "summary": "Get product event history",
        "tags": [
          "products"
        ]
      }
    }
  },
  "servers": [],
  "tags": [
    {
      "description": "Product management endpoints",
      "name": "products"
    }
  ]
}