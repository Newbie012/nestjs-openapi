{
  "components": {
    "schemas": {
      "CreateProductDto": {
        "description": "DTO for creating a product with array validation",
        "properties": {
          "description": {
            "maxLength": 1000,
            "type": "string"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "price": {
            "exclusiveMinimum": 0,
            "type": "number"
          },
          "stock": {
            "minimum": 0,
            "type": "integer"
          },
          "tags": {
            "items": {
              "type": "string"
            },
            "maxItems": 10,
            "minItems": 1,
            "type": "string"
          }
        },
        "required": [
          "name",
          "price",
          "tags"
        ],
        "type": "object"
      },
      "CreateUserDto": {
        "description": "DTO for creating a user - demonstrates various validation decorators",
        "properties": {
          "age": {
            "maximum": 150,
            "minimum": 0,
            "type": "integer"
          },
          "bio": {
            "type": "string"
          },
          "email": {
            "format": "email",
            "type": "string"
          },
          "name": {
            "maxLength": 50,
            "minLength": 2,
            "type": "string"
          },
          "password": {
            "maxLength": 128,
            "minLength": 8,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)",
            "type": "string"
          },
          "role": {
            "$ref": "#/components/schemas/UserRole",
            "enum": [
              "admin",
              "user",
              "guest"
            ]
          },
          "roleAsString": {
            "description": "This property type is string, but",
            "enum": [
              "admin",
              "user",
              "guest"
            ],
            "type": "string"
          },
          "status": {
            "$ref": "#/components/schemas/UserStatus",
            "enum": [
              0,
              1,
              2
            ]
          },
          "website": {
            "format": "uri",
            "type": "string"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "age",
          "role",
          "roleAsString"
        ],
        "type": "object"
      },
      "PaginationDto": {
        "description": "DTO for pagination query parameters",
        "properties": {
          "limit": {
            "maximum": 100,
            "minimum": 1,
            "type": "integer"
          },
          "page": {
            "minimum": 1,
            "type": "integer"
          }
        },
        "type": "object"
      },
      "ProductResponseDto": {
        "description": "DTO for product response",
        "properties": {
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "id": {
            "format": "uuid",
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "price": {
            "exclusiveMinimum": 0,
            "type": "number"
          },
          "stock": {
            "minimum": 0,
            "type": "integer"
          },
          "tags": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "id",
          "name",
          "description",
          "price",
          "tags",
          "stock",
          "createdAt"
        ],
        "type": "object"
      },
      "UpdateUserDto": {
        "description": "DTO for updating a user - all fields optional",
        "properties": {
          "age": {
            "maximum": 150,
            "minimum": 0,
            "type": "integer"
          },
          "email": {
            "format": "email",
            "type": "string"
          },
          "isActive": {
            "type": "boolean"
          },
          "name": {
            "maxLength": 50,
            "minLength": 2,
            "type": "string"
          },
          "website": {
            "format": "uri",
            "type": "string"
          }
        },
        "type": "object"
      },
      "UserResponseDto": {
        "description": "DTO representing a user response",
        "properties": {
          "age": {
            "type": "integer"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "email": {
            "format": "email",
            "type": "string"
          },
          "id": {
            "format": "uuid",
            "type": "string"
          },
          "isActive": {
            "type": "boolean"
          },
          "name": {
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          },
          "website": {
            "format": "uri",
            "type": "string"
          }
        },
        "required": [
          "id",
          "name",
          "email",
          "age",
          "isActive",
          "createdAt",
          "updatedAt"
        ],
        "type": "object"
      },
      "UserRole": {
        "description": "User role enum for testing",
        "enum": [
          "admin",
          "user",
          "guest"
        ],
        "type": "string"
      },
      "UserStatus": {
        "description": "User status with numeric values",
        "enum": [
          0,
          1,
          2
        ],
        "type": "number"
      }
    }
  },
  "info": {
    "description": "API demonstrating DTO validation with class-validator",
    "title": "DTO Validation API",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {
    "/products": {
      "get": {
        "operationId": "ProductController_findAll",
        "parameters": [],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/ProductResponseDto"
                  },
                  "type": "array"
                }
              }
            },
            "description": "List of products"
          }
        },
        "summary": "Get all products",
        "tags": [
          "Products"
        ]
      },
      "post": {
        "operationId": "ProductController_create",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateProductDto"
              }
            }
          },
          "description": "Request body parameter: _createProductDto",
          "required": true
        },
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductResponseDto"
                }
              }
            },
            "description": "Product created"
          },
          "400": {
            "description": "Validation failed"
          }
        },
        "summary": "Create a new product with validation",
        "tags": [
          "Products"
        ]
      }
    },
    "/products/{id}": {
      "get": {
        "operationId": "ProductController_findOne",
        "parameters": [
          {
            "description": "Product UUID",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductResponseDto"
                }
              }
            },
            "description": "Product found"
          },
          "404": {
            "description": "Product not found"
          }
        },
        "summary": "Get a product by ID",
        "tags": [
          "Products"
        ]
      }
    },
    "/users": {
      "get": {
        "operationId": "UserController_findAll",
        "parameters": [
          {
            "description": "query parameter: _pagination",
            "in": "query",
            "name": "_pagination",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/PaginationDto"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/UserResponseDto"
                  },
                  "type": "array"
                }
              }
            },
            "description": "List of users"
          }
        },
        "summary": "Get all users with pagination",
        "tags": [
          "Users"
        ]
      },
      "post": {
        "operationId": "UserController_create",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUserDto"
              }
            }
          },
          "description": "Request body parameter: _createUserDto",
          "required": true
        },
        "responses": {
          "201": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserResponseDto"
                }
              }
            },
            "description": "User created"
          },
          "400": {
            "description": "Validation failed"
          }
        },
        "summary": "Create a new user with validation",
        "tags": [
          "Users"
        ]
      }
    },
    "/users/{id}": {
      "get": {
        "operationId": "UserController_findOne",
        "parameters": [
          {
            "description": "User UUID",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserResponseDto"
                }
              }
            },
            "description": "User found"
          },
          "404": {
            "description": "User not found"
          }
        },
        "summary": "Get a user by ID",
        "tags": [
          "Users"
        ]
      },
      "put": {
        "operationId": "UserController_update",
        "parameters": [
          {
            "description": "User UUID",
            "in": "path",
            "name": "id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateUserDto"
              }
            }
          },
          "description": "Request body parameter: _updateUserDto",
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserResponseDto"
                }
              }
            },
            "description": "User updated"
          },
          "400": {
            "description": "Validation failed"
          },
          "404": {
            "description": "User not found"
          }
        },
        "summary": "Update a user",
        "tags": [
          "Users"
        ]
      }
    }
  },
  "servers": [],
  "tags": []
}